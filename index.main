    <link href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fortawesome/fontawesome-free@6.4.0/css/all.min.css">
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700&display=swap" rel="stylesheet">
    <style>
        body {
            font-family: 'Roboto', sans-serif;
            background-color: #f0f4f8;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            min-height: 100vh;
            margin: 0;
            overflow: hidden; /* Hide scrollbars to maintain fixed slide position */
        }
        .presentation-container {
            position: relative;
            width: 100%;
            height: 100vh; /* Take full viewport height */
            display: flex;
            justify-content: center;
            align-items: center;
        }
        .slide {
            background-color: white;
            border-radius: 8px;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
            padding: 40px;
            position: absolute; /* Position slides absolutely within the container */
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%); /* Center the slide */
            width: 90%; /* Adjust width as needed */
            max-width: 960px; /* Max width for readability */
            height: 90%; /* Adjust height as needed */
            max-height: 700px; /* Max height for readability */
            overflow-y: auto; /* Enable scrolling for long content */
            display: none; /* Hidden by default */
            transition: opacity 0.5s ease-in-out;
            opacity: 0;
        }
        .slide.active {
            display: block;
            opacity: 1;
        }
        .slide-header {
            color: #1e40af;
            margin-bottom: 1.5rem;
        }
        .slide-number {
            position: absolute;
            bottom: 20px;
            right: 20px;
            font-size: 14px;
            color: #6b7280;
        }
        .feature-box {
            background-color: #f0f7ff;
            border-left: 4px solid #1e40af;
            padding: 15px;
            margin-bottom: 15px;
            border-radius: 5px;
        }
        .process-arrow {
            position: relative;
            margin: 0 15px;
            color: #1e40af;
        }
        .timeline-item {
            position: relative;
            padding-left: 30px;
            margin-bottom: 20px;
        }
        .timeline-item:before {
            content: "";
            position: absolute;
            left: 0;
            top: 5px;
            width: 15px;
            height: 15px;
            border-radius: 50%;
            background: #1e40af;
        }
        .timeline-item:after {
            content: "";
            position: absolute;
            left: 7px;
            top: 20px;
            height: calc(100% + 10px);
            border-left: 2px solid #1e40af;
        }
        .timeline-item:last-child:after {
            display: none;
        }
        .school-logo {
            height: 60px;
            margin-bottom: 20px;
        }
        .hero-section {
            background: linear-gradient(135deg, #1e3a8a 0%, #3b82f6 100%);
            color: white;
            padding: 60px 40px;
            border-radius: 8px;
            margin-bottom: 30px;
        }
        table {
            width: 100%;
            border-collapse: collapse;
        }
        table th, table td {
            border: 1px solid #e5e7eb;
            padding: 10px;
            text-align: left;
        }
        table th {
            background-color: #f0f7ff;
            color: #1e40af;
        }
        .navigation-controls {
            position: fixed;
            bottom: 20px;
            left: 50%;
            transform: translateX(-50%);
            display: flex;
            gap: 20px;
            z-index: 1000; /* Ensure controls are above slides */
            align-items: center; /* Align items vertically */
        }
        .nav-btn {
            background-color: #1e40af;
            color: white;
            padding: 10px 20px;
            border-radius: 5px;
            cursor: pointer;
            font-size: 16px;
            display: flex;
            align-items: center;
            gap: 8px;
            transition: background-color 0.3s ease;
        }
        .nav-btn:hover:not(:disabled) {
            background-color: #3b82f6;
        }
        .nav-btn:disabled {
            background-color: #9ca3af;
            cursor: not-allowed;
        }
        .progress-bar-container {
            width: 80%;
            max-width: 960px;
            background-color: #e5e7eb;
            height: 8px;
            border-radius: 4px;
            margin-bottom: 20px;
            position: fixed; /* Fixed at the top */
            top: 20px;
            left: 50%;
            transform: translateX(-50%);
            z-index: 1000;
        }
        .progress-bar {
            height: 100%;
            width: 0%;
            background-color: #1e40af;
            border-radius: 4px;
            transition: width 0.5s ease-in-out;
        }
        .audio-playback-controls {
            display: flex;
            align-items: center;
            gap: 10px;
            color: #1e40af;
            font-size: 14px;
            cursor: pointer;
        }
        .audio-playback-controls button {
            background: none;
            border: none;
            color: inherit;
            font-size: inherit;
            cursor: pointer;
            padding: 0;
            display: flex;
            align-items: center;
        }
        .audio-playback-controls button:hover {
            color: #3b82f6;
        }
    </style>
</head>
