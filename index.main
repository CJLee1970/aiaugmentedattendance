    <div class="navigation-controls">
        <button id="prevBtn" class="nav-btn">
            <i class="fas fa-chevron-left"></i> Previous
        </button>
        <div class="audio-playback-controls">
            <button id="playPauseBtn">
                <i class="fas fa-volume-up mr-2"></i> Audio
            </button>
        </div>
        <button id="nextBtn" class="nav-btn">
            Next <i class="fas fa-chevron-right"></i>
        </button>
    </div>

    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const slides = document.querySelectorAll('.slide');
            const totalSlides = slides.length;
            let currentSlideIndex = 0;
            const prevBtn = document.getElementById('prevBtn');
            const nextBtn = document.getElementById('nextBtn');
            const playPauseBtn = document.getElementById('playPauseBtn');
            const progressBar = document.getElementById('progressBar');
            let currentAudio = null;

            function updateProgressBar() {
                const progress = ((currentSlideIndex + 1) / totalSlides) * 100;
                progressBar.style.width = `${progress}%`;
            }

            function showSlide(index) {
                // Stop current audio if playing
                if (currentAudio) {
                    currentAudio.pause();
                    currentAudio.currentTime = 0;
                    playPauseBtn.innerHTML = '<i class="fas fa-volume-up mr-2"></i> Audio'; // Reset button text
                }

                slides.forEach((slide, i) => {
                    if (i === index) {
                        slide.classList.add('active');
                        slide.classList.remove('hidden'); // Ensure it's not hidden by other means
                        const slideNumberDiv = slide.querySelector('.slide-number');
                        if (slideNumberDiv) {
                            slideNumberDiv.textContent = `${index + 1}/${totalSlides}`;
                        }

                        // Set current audio for the active slide
                        currentAudio = slide.querySelector('audio');
                        if (currentAudio) {
                            const playPromise = currentAudio.play();
                            if (playPromise !== undefined) {
                                playPromise.then(_ => {
                                    // Autoplay started!
                                    playPauseBtn.innerHTML = '<i class="fas fa-pause mr-2"></i> Pause Audio';
                                    playPauseBtn.disabled = false; // Enable button if it was disabled
                                }).catch(error => {
                                    // Autoplay failed. This is likely due to user gesture requirement.
                                    console.log('Autoplay prevented:', error);
                                    playPauseBtn.innerHTML = '<i class="fas fa-play mr-2"></i> Play Audio';
                                    playPauseBtn.disabled = false; // Ensure button is enabled for manual play
                                });
                            }
                        } else {
                            playPauseBtn.innerHTML = '<i class="fas fa-volume-mute mr-2"></i> No Audio';
                            playPauseBtn.disabled = true;
                        }

                    } else {
                        slide.classList.remove('active');
                        slide.classList.add('hidden'); // Hide non-active slides
                    }
                });

                prevBtn.disabled = index === 0;
                nextBtn.disabled = index === totalSlides - 1;
                updateProgressBar();
            }

            function toggleAudioPlayPause() {
                if (currentAudio) {
                    if (currentAudio.paused) {
                        currentAudio.play();
                        playPauseBtn.innerHTML = '<i class="fas fa-pause mr-2"></i> Pause Audio';
                    } else {
                        currentAudio.pause();
                        playPauseBtn.innerHTML = '<i class="fas fa-play mr-2"></i> Play Audio';
                    }
                }
            }

            function nextSlide() {
                if (currentSlideIndex < totalSlides - 1) {
                    currentSlideIndex++;
                    showSlide(currentSlideIndex);
                }
            }

            function prevSlide() {
                if (currentSlideIndex > 0) {
                    currentSlideIndex--;
                    showSlide(currentSlideIndex);
                }
            }

            // Event Listeners
            nextBtn.addEventListener('click', nextSlide);
            prevBtn.addEventListener('click', prevSlide);
            playPauseBtn.addEventListener('click', toggleAudioPlayPause);

            // Optional: Keyboard navigation
            document.addEventListener('keydown', (e) => {
                if (e.key === 'ArrowRight') {
                    nextSlide();
                } else if (e.key === 'ArrowLeft') {
                    prevSlide();
                }
            });

            // Initial slide display
            showSlide(currentSlideIndex);
        });
    </script>
</body>
